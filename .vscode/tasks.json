{
    "command": "gcc",
    "args": [
        "rpoly.c",
        "MatLabUtilites.c",
        "MatLabUtilitesTest.c",
        "-o",
        "matlabutilites.out",
        "-lm",
        "&&",
        "./matlabutilites.out",
        "&&",
        "gcc",
        "rpoly.c",
        "MatLabUtilites.c",
        "unit.c",
        "doubler.c",
        "angl.c",
        "newtonnu.c",
        "R_x.c",
        "R_y.c",
        "R_z.c",
        "Frac.c",
        "timediff.c",
        "Mjday.c",
        "Position.c",
        "MeanObliquity.c",
        "NutAngles.c",
        "gmst.c",
        "gibbs.c",
        "EqnEquinox.c",
        "IERS.c",
        "gast.c",
        "NutMatrix.c",
        "PoleMatrix.c",
        "PreliminaryOrbitDeterminationTest.c",
        "-o",
        "PreliminaryOrbitDeterminationTest.out",
        "-lm",
        "&&",
        "./PreliminaryOrbitDeterminationTest.out",
        "&&",
        "rm",
        "matlabutilites.out",
        "&&",
        "rm",
        "PreliminaryOrbitDeterminationTest.out"
      ],
    "isShellCommand": true,
    "showOutput": "always",
    "problemMatcher": {
        "owner": "c",
        "fileLocation": [
            "relative",
            "${workspaceRoot}"
        ],
        "pattern": {
            "regexp": "^(.*):(\\d+):(\\d+):\\s+(warning|error):\\s+(.*)$",
            "file": 1,
            "line": 2,
            "column": 3,
            "severity": 4,
            "message": 5
        }
    }
}
